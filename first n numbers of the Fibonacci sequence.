function generateFibonacci(n) {
    // Initialize the Fibonacci sequence with the first two numbers
    let fibonacciSequence = [0, 1];

    // Generate the sequence up to n numbers
    for (let i = 2; i < n; i++) {
        let nextNumber = fibonacciSequence[i - 1] + fibonacciSequence[i - 2];
        fibonacciSequence.push(nextNumber);
    }

    // If n is 1, return only the first number
    if (n === 1) {
        return [0];
    }

    // Return the sequence up to n numbers
    return fibonacciSequence.slice(0, n);
}

console.log(generateFibonacci(16));  
console.log(generateFibonacci(24)); 
console.log(generateFibonacci(12));  
console.log(generateFibonacci(6));  
